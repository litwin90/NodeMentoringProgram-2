{
  "$ref": "#/definitions/CreateActionGroup",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "CreateActionGroup": {
      "$ref": "#/definitions/Optional%3CIGroup%2C%22id%22%3E"
    },
    "Optional<IGroup,\"id\">": {
      "additionalProperties": false,
      "description": "Type helper for making certain fields of an object optional. This is helpful\nfor creating the `CreationAttributes` from your `Attributes` for a Model.",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "maxLength": 24,
          "minLength": 5,
          "type": "string"
        },
        "permissions": {
          "items": {
            "$ref": "#/definitions/Permission"
          },
          "type": "array"
        }
      },
      "required": [
        "name",
        "permissions"
      ],
      "type": "object"
    },
    "Permission": {
      "enum": [
        "READ",
        "WRITE",
        "DELETE",
        "SHARE",
        "UPLOAD_FILES"
      ],
      "type": "string"
    }
  }
}