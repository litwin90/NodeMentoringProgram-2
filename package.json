{
    "name": "nodementoringprogram-2",
    "version": "1.0.0",
    "description": "NMP-2",
    "main": "index.js",
    "scripts": {
        "start": "nodemon  src/app.ts",
        "generate-initial-data": "nodemon scripts/generate-initial-data.ts",
        "test": "test",
        "tsc": "tsc",
        "lint": "eslint . --ext .ts",
        "generate-json-schema:All": "npm-run-all generate-json-schema:User generate-json-schema:Group",
        "generate-json-schema:User": "npm-run-all generate-json-schema:UpdateActionUser generate-json-schema:CreateActionUser",
        "generate-json-schema:Group": "npm-run-all generate-json-schema:UpdateActionGroup generate-json-schema:CreateActionGroup",
        "generate-json-schema:UpdateActionUser": "ts-json-schema-generator --no-type-check --path src/interfaces/IUser.ts -t UpdateActionUser > src/schemas/UpdateActionUser.json",
        "generate-json-schema:CreateActionUser": "ts-json-schema-generator --no-type-check --path src/interfaces/IUser.ts -t CreateActionUser > src/schemas/CreateActionUser.json",
        "generate-json-schema:UpdateActionGroup": "ts-json-schema-generator --no-type-check --path src/interfaces/IGroup.ts -t UpdateActionGroup > src/schemas/UpdateActionGroup.json",
        "generate-json-schema:CreateActionGroup": "ts-json-schema-generator --no-type-check --path src/interfaces/IGroup.ts -t CreateActionGroup > src/schemas/CreateActionGroup.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/litwin90/NodeMentoringProgram-2.git"
    },
    "author": "Dzianis Davydau",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/litwin90/NodeMentoringProgram-2/issues"
    },
    "homepage": "https://github.com/litwin90/NodeMentoringProgram-2#readme",
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/ajv-merge-patch": "^4.1.0",
        "@types/cls-hooked": "^4.3.1",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.7",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.10.1",
        "@types/pg": "^7.14.4",
        "@types/sequelize": "^4.28.9",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "ts-json-schema-generator": "^0.72.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "@types/moment": "^2.13.0",
        "@types/nodemon": "^1.19.0",
        "@types/winston": "^2.4.4",
        "ajv": "^6.12.4",
        "cls-hooked": "^4.2.2",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "pg": "^8.3.3",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.3.5",
        "typedi": "^0.8.0",
        "uuid": "^8.3.0",
        "winston": "^3.3.3"
    }
}
